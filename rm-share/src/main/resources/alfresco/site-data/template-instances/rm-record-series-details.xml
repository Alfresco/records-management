<?xml version='1.0' encoding='UTF-8'?>
<template-instance>
   <template-type>org/alfresco/rm/record-series-details</template-type>
   <properties>
      <pageFamily>documentlibrary</pageFamily>
      <container>documentLibrary</container>
   </properties>
   <components>
      <component>
         <region-id>title</region-id>
         <url>/components/title/collaboration-title</url>
      </component>
      <component>
         <region-id>navigation</region-id>
         <url>/components/navigation/collaboration-navigation</url>
      </component>
      <component>
         <region-id>actions-common</region-id>
         <url>/rm/components/documentlibrary/actions-common</url>
      </component>
      <component>
         <region-id>path</region-id>
         <url>/rm/components/folder-details/path</url>
      </component>
      <component>
         <region-id>folder-metadata-header</region-id>
         <url>/components/folder-details/folder-metadata-header</url>
      </component>
      <component>
         <region-id>folder-metadata</region-id>
         <url>/components/form</url>
         <properties>
            <itemKind>node</itemKind>
            <itemId>{nodeRef}</itemId>
            <formId>rm</formId>
            <mode>view</mode>
         </properties>
      </component>
      <component>
         <region-id>folder-actions</region-id>
         <url>/rm/components/folder-details/folder-actions</url>
      </component>
      <component>
         <region-id>folder-links</region-id>
         <url>/components/folder-details/folder-links</url>
      </component>
   </components>
</template-instance>
